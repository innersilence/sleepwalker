<Window x:Class="DataMonitor.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d3="http://research.microsoft.com/DynamicDataDisplay/1.0"
        
    Title="Data Monitor" WindowState="Normal" Height="500" Width="900">
    
    <Grid>
        <d3:ChartPlotter Name="plotter" Margin="20,40,40,20">
            <d3:LineGraph DataSource="{Binding Path=MonitorData}" Stroke="Green" />
        </d3:ChartPlotter>
    </Grid>
</Window>
